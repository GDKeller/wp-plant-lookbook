{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "outDir": "./dist",
        "allowJs": false,
        "jsx": "react-jsx",
        "baseUrl": "./src",
        "paths": {
            "@blocks/*": ["blocks/*"]
        }
    },
"include": ["src/**/*.ts", "src/blocks/species/index.tsx"],
"exclude": ["node_modules", "dist"]
}