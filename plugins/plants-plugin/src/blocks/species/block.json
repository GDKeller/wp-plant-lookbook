{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "plants-plugin/species",
	"title": "Species",
	"category": "custom-blocks",
	"description": "A block to display species information.",
	"example": {},
	"supports": {
		"html": false
	},
	"editorScript": "plants-plugin/species/block",
	"editorStyle": "plants-plugin/species/editor",
	"style": "plants-plugin/species/style",
	"render": "../../../render/block-species.php",
	"attributes": {
		"flipped": {
			"type": "boolean",
			"default": false
		},
		"speciesFormalName": {
			"type": "string",
			"default": ""
		},
		"speciesCommonName": {
			"type": "string",
			"default": ""
		},
		"speciesDescription": {
			"type": "string",
			"default": ""
		},
		"speciesImage": {
			"type": "object",
			"default": {}
		}
	}
}
